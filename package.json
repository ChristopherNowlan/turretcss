{
  "name": "turretcss",
  "version": "4.0.0-rc2",
  "description": "Developed for design. Turret is a styles and browser behaviour normalisation framework for rapid development of responsive and accessible websites.",
  "homepage": "http://turretcss.com/",
  "author": "Bigfish.tv",
  "license": "MIT",
  "style": "turret/turret.css",
  "contributors": [
    {
      "name": "Scott de Jonge",
      "email": "scottdj@bigfish.tv",
      "url": "http://scottdejonge.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bigfishtv/turret"
  },
  "bugs": {
    "url": "https://github.com/bigfishtv/turret/issues"
  },
  "keywords": [
    "css",
    "postcss",
    "cssnext",
    "a11y",
    "html",
    "responsive",
    "front-end",
    "framework",
    "web"
  ],
  "scripts": {
    "start": "npm run watch:docs",
    "watch:docs": "npm run docs -- -w",
    "watch:examples": "npm run examples -- -w",
    "build": "postcss turret/turret.css -o dist/turret.min.css",
    "docs": "postcss docs/assets/css/main.css -o docs/dist/docs.css",
    "examples": "npm run examples:bold -- -w",
    "examples:bold": "postcss docs/examples/bold/css/input.css -o docs/examples/bold/css/output.css"
  },
  "devDependencies": {
    "cssnano": "^3.10.0",
    "postcss-cli": "^3.0.0-beta",
    "postcss-cssnext": "^2.8.0",
    "postcss-import": "^8.1.2"
  },
  "peerDependencies": {
    "postcss-cssnext": "^2.8.0",
    "postcss-import": "^8.1.2"
  }
}
